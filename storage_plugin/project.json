{
  "$schema": "https://cdn.jsdelivr.net/gh/Byzanteam/jcli/schemas/project-file.v1.json",
  "name": "storage_plugin",
  "title": "storage_plugin",
  "capabilities": [
    {
      "name": "storage_plugin_database",
      "payload": {
        "__type__": "database",
        "schema": "storage_plugin"
      }
    }
  ],
  "instances": [
    {
      "capabilityNames": [
        "storage_plugin_database"
      ],
      "config": {
        "buckets": [
          {
            "name": "jet-storage-plugin-example",
            "endpoint": "https://nightly.minio.apps.jet.work",
            "accessKey": "{{ ACCESS_KEY }}",
            "secretKey": "{{ SECRET_KEY }}"
          }
        ]
      },
      "name": "storage",
      "pluginName": "storage"
    }
  ]
}
