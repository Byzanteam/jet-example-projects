{
  "$schema": "https://deno.land/x/deno/cli/schemas/config-file.v1.json",
  "tasks": {
    "build": "deno run -A scripts/build.ts"
  },
  "compilerOptions": {
    "strict": true,
    "useUnknownInCatchVariables": true,
    "noImplicitOverride": true,
    "types": ["https://cdn.jsdelivr.net/gh/Byzanteam/breeze-js@latest/mod.d.ts"]
  },
  "imports": {
    "@byzanteam/breeze-js": "jsr:@byzanteam/breeze-js@^0.4.0",
    "@hono/hono": "jsr:@hono/hono@4.6.3",
    "kysely-deno-postgres-dialect/": "https://deno.land/x/kysely_postgrs_js_dialect@v0.27.5/",
    "postgresjs/": "https://deno.land/x/postgresjs@v3.4.4/",
    "kysely/": "https://cdn.jsdelivr.net/npm/kysely@0.27.3/dist/esm/"
  }
}
